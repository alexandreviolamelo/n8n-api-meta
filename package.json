{
  "name": "n8n-nodes-meta-integration-advanced",
  "version": "1.2.2",
  "description": "Advanced Meta Platforms integration for n8n - Instagram, Facebook, WhatsApp with enhanced features",
  "keywords": [
    "n8n-community-node-package",
    "instagram",
    "meta",
    "facebook",
    "whatsapp",
    "chatbot",
    "automation",
    "crm",
    "advanced"
  ],
  "license": "MIT",
  "author": {
    "name": "Alexandre Viola Melo",
    "email": "alexandreviolamelo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandreviolamelo/n8n-api-meta.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p dist && cp -r nodes dist/ && cp -r credentials dist/ && cp package.json dist/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MetaApi.credentials.js",
      "dist/credentials/Advanced/WhatsAppBusinessApi.credentials.js",
      "dist/credentials/InstagramApi.credentials.js",
      "dist/credentials/MetaOAuth2Api.credentials.js",
      "dist/credentials/MetaAccessToken.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Instagram/InstagramTrigger.node.js",
      "dist/nodes/Meta/InstagramTrigger.node.js",
      "dist/nodes/Meta/Advanced/InstagramAdvanced.node.js",
      "dist/nodes/Facebook/Pages/FacebookPages.node.js",
      "dist/nodes/WhatsApp/Business/WhatsAppBusiness.node.js",
      "dist/nodes/Meta/Instagram.node.js",
      "dist/nodes/Meta/Facebook.node.js",
      "dist/nodes/Meta/FacebookTrigger.node.js",
      "dist/nodes/Meta/WhatsApp.node.js",
      "dist/nodes/Meta/WhatsAppTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "gulp": "^4.0.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "axios": "^1.6.0",
    "mailchimp-api-v3": "^1.15.0",
    "n8n-api-meta": "^1.0.1",
    "openai": "^5.13.1",
    "sentiment": "^5.0.2"
  }
}